MDScreen:
	name: "home"

	MDFloatLayout:
		md_bg_color: get_color_from_hex("#1E1F24")

		ScrollView:
			do_scroll_x: False
			do_scroll_y: True
			pos_hint: {"x": 0, "y": 0}

			MDFloatLayout:
				size_hint_y: None
				size: (360, 1900)
				md_bg_color: get_color_from_hex("#1E1F24")

				Image: 
					source: "Items/avatar.png"
					pos_hint: {"x": 0.027, "y": 0.975}
					size_hint: (0.11, 0.021)
				MDLabel:
					text: "Gustavo"
					size_hint: (0.22, 0.012)
					pos_hint: {"x": 0.16, "y": 0.98}
					color: get_color_from_hex("#E4E4E4")

		MDBoxLayout:
			md_bg_color: get_color_from_hex("#23232D")
			size_hint: (1, 0.07)
			pos_hint: {"x": 0, "y": 0}




















































































